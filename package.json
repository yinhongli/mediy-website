{
  "name": "mediy-website",
  "version": "1.0.0",
  "description": "MEDIY 医疗科技官网",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "vite",
    "server": "node server/index.js",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/index.js",
    "pm2:start": "pm2 start ecosystem.config.js --only mediy-website",
    "pm2:start:dev": "pm2 start ecosystem.config.js --only mediy-website-dev",
    "pm2:stop": "pm2 stop mediy-website",
    "pm2:restart": "pm2 restart mediy-website",
    "pm2:delete": "pm2 delete mediy-website",
    "pm2:logs": "pm2 logs mediy-website",
    "pm2:status": "pm2 status",
    "pm2:prod": "npm run build && pm2 start ecosystem.config.js --only mediy-website",
    "docker:build": "docker build -t mediy-website:latest .",
    "docker:run": "docker run -d --name mediy-website -p 3002:3002 -v $(pwd)/data:/app/data mediy-website:latest",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:rebuild": "docker-compose up -d --build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "vite": "^4.5.0",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "react",
    "tailwindcss",
    "medical",
    "website"
  ],
  "author": "MEDIY",
  "license": "MIT",
  "overrides": {
    "postcss-load-config": "5.0.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}